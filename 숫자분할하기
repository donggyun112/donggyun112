#include <iostream>
using namespace std;

int main()
{
	int a;
	cin >> a;
	for (int x = 0; x < 100; x++) {
		if (a < 1000 || a>9999) {
			cout << "1000 ~ 9999 사이의 숫자를 입력해주세요" << endl;
			cin >> a;
		}
		else {
			break;
		}
	}


	int v[4];
	v[0] = a / 1000;
	 v[1] = (a % 1000) / 100;
	 v[2] = ((a % 1000) % 100) / 10;
	 v[3] = ((a % 1000) % 100) % 10;

	 for (int x = 0; x < 4; x++) {
		 cout << "숫자" << v[x] << '\n';
	}





	return 0;
}
