#include <iostream>
using namespace std;

int main()
{
	int v[2][3] = {
		1,3,4,
		0,1,3
	};
	int v1[5] = { 1,1,3,0,1 };
	
	int t;
	for (int y = 0; y < 5; y++) {
		t = 0;
		for (int yz = 0; yz < 2; yz++) {
			for (int x = 0; x < 3; x++) {
				if (v1[y] == v[yz][x]) {
					t++;
				}
			}
		}
		cout << t << " ";
	}

 


	return 0;

}
