#include <iostream>
using namespace std;

int main()
{
	int v[3][5] = {
		1,1,3,0,1,
		0,1,7,9,3,
		9,8,6,5,15
	};
	int max;
	int min;

	for (int y = 0; y < 3; y++) {
		min = 99;
		for (int x = 0; x < 5; x++) {
			if (v[y][x] < min) {
				min = v[y][x];
			}
		}
		cout << min << endl;
	}
	cout << "---------------------" << endl;

	for (int x = 0; x < 5; x++) {
		max = 0;
		for (int y = 0; y < 3; y++) {
			if (v[y][x] > max) {
				max = v[y][x];
			}
		}
		cout << max << " ";
	}






	return 0;
}
